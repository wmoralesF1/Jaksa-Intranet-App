<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_marginVertical="5dp"
        android:paddingHorizontal="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tvIdViajeListTicketTravel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="15"
            android:textSize="12sp"></TextView>
        <TextView
            android:id="@+id/tvNumBusListTicketTravel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="10"
            android:textSize="12sp"></TextView>
        <TextView
            android:id="@+id/tvNumBoletoListTicketTravel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="20"
            android:textSize="12sp"></TextView>
        <TextView
            android:id="@+id/tvPrecioListTicketTravel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="15"
            android:textSize="12sp"></TextView>
        <TextView
            android:id="@+id/tvNumAsientoListTicketTravel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="10"
            android:textSize="12sp"></TextView>
        <TextView
            android:id="@+id/tvEstadoListTicketTravel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="20"
            android:textSize="12sp"></TextView>
    </LinearLayout>
</LinearLayout>